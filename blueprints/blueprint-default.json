{
  "$schema": "https://playground.wordpress.net/blueprint-schema.json",
  "meta": {
    "title": "Stylish Press",
    "description": "A Woo store with custom theme, content, and products.",
    "author": "adamziel",
    "categories": ["Woocommerce", "Site"]
  },
  "landingPage": "/wp-admin",
  "preferredVersions": {
    "php": "8.3",
    "wp": "latest"
  },
  "features": {
    "networking": true
  },
  "steps": [
    {
      "step": "installPlugin",
      "pluginData": {
        "resource": "url",
        "url": "https://github-proxy.com/proxy/?repo=qcausa/wp-job-board-pro&branch=main"
      },
      "options": {
        "activate": true
      }
    },
    {
      "step": "installPlugin",
      "pluginData": {
        "resource": "url",
        "url": "https://github-proxy.com/proxy/?repo=qcausa/wp-job-board-pro-partners&branch=main"
      },
      "options": {
        "activate": true
      }
    },
    {
      "step": "installPlugin",
      "pluginData": {
        "resource": "wordpress.org/plugins",
        "slug": "bugfu-console-debugger"
      },
      "options": {
        "activate": true
      }
    },
    {
      "step": "writeFile",
      "path": "/tmp/wp_job_board_pro_options.sql",
      "data": {
        "resource": "url",
        "url": "https://raw.githubusercontent.com/qcausa/wp-job-board-pro-partners/refs/heads/main/blueprints/wp_job_board_pro_options.sql"
      }
    },
    {
      "step": "wp-cli",
      "command": "wp db query < /tmp/wp_job_board_pro_options.sql"
    },
    {
      "step": "wp-cli",
      "command": "wp post generate --count=5 --post_type=candidate --post_date=1999-01-04"
    },
    {
      "step": "wp-cli",
      "command": "wp post generate --count=5 --post_type=employer --post_date=1999-01-04"
    },
    {
      "step": "wp-cli",
      "command": "wp post generate --count=5 --post_type=partner --post_date=1999-01-04"
    },
    {
      "step": "writeFile",
      "path": "/wordpress/wp-content/mu-plugins/rewrite.php",
      "data": "<?php add_action( 'after_setup_theme', function() { global $wp_rewrite; $wp_rewrite->set_permalink_structure('/%postname%/'); $wp_rewrite->flush_rules(); } );"
    },
    {
      "step": "login",
      "username": "admin",
      "password": "password"
    }
  ]
}
